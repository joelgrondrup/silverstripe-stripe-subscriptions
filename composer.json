{
  "name": "joelgrondrup/silverstripe-stripe-subscriptions",
  "description": "A modules for creating subscriptions via Stripe",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/joelgrondrup/silverstripe-stripe-subscriptions",
  "keywords": ["silverstripe", "subscription", "saas", "subscriptions", "stripe"],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Joel Gr√∏ndrup","email": "joel@joel.biz"
    }
  ],
  "support": {
    "issues": "https://github.com/joelgrondrup/silverstripe-stripe-subscriptions"
  },
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/joelgrondrup/silverstripe-stripewebhook"
      }
  ],
  "require": {
        "php": "^8.1",
        "silverstripe/cms": "^5.4",
        "silverstripe/framework": "^5.4",
        "joelgrondrup/silverstripe-stripewebhook": "dev-master",
        "silverstripe/linkfield": "^4.0"
    },
  "autoload": {
    "psr-4": {
      "JoelGrondrup\\StripeSubscriptions\\": "src/"
    }
  },
  "extra": {
    "installer-name": "silverstripe-microbizz",
    "expose": [
      "client"
    ],
    "screenshots": [
      
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
